{
    "version": 2,
    "name": "Playing",
    "env": {
        "API_KEY": "process.env.API_KEY"
    },
    "icons": [
      {
        "src": "favicon.ico",
        "sizes": "96x96 64x64 32x32 24x24 16x16",
        "type": "image/x-icon"
      }
    ],
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "^/manifest.json$",
          "dest": "/manifest.json"
         },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ]
  }